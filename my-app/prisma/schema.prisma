generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Income {
  id     Int    @id @default(autoincrement())
  userId String
  source String
  freq   String
  amount Int
}

model Expense {
  id     Int    @id @default(autoincrement())
  userId String
  source String
  freq   String
  amount Int
}
